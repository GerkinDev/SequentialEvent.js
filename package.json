{
  "name": "sequential-event",
  "version": "0.2.0",
  "description": "This library is a variation of standard event emitters. Handlers are executed sequentialy, and may return Promises if it executes asynchronous code",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "test": "mocha test/index.js",
    "test-coverage": "nyc --reporter=lcov mocha test/index.js",
    "dist": "grunt dist && grunt documentate",
    "build-tests": "grunt refreshTests",
    "build-then-check": "npm run dist && npm run build-tests && npm run test",
    "preversion": "script/deploy",
    "version": "npm run build-then-check && git add ."
  },
  "author": "Gerkin",
  "license": "GPL-3.0",
  "devDependencies": {
    "@comandeer/babel-plugin-banner": "^1.0.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "chai": "^4.1.2",
    "eslint-plugin-jsdoc": "^3.1.3",
    "expect.js": "^0.3.1",
    "grunt": "^1.0.1",
    "grunt-babel": "^7.0.0",
    "grunt-browserify": "^5.2.0",
    "grunt-contrib-uglify": "^3.1.0",
    "grunt-jsdoc": "^2.2.0",
    "gruntify-eslint": "^4.0.0",
    "ink-docstrap": "^1.3.0",
    "load-grunt-tasks": "^3.5.2",
    "lodash": "^4.17.4",
    "mocha": "^4.0.1",
    "nyc": "^11.2.1",
    "saucelabs": "^1.4.0",
    "selenium-webdriver": "^3.6.0"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GerkinDev/SequentialEvent.js.git"
  },
  "keywords": [
    "Promise",
    "EventEmitter",
    "Control-Flow"
  ],
  "bugs": {
    "url": "https://github.com/GerkinDev/SequentialEvent.js/issues"
  },
  "homepage": "https://gerkindev.github.io/SequentialEvent.js/"
}
